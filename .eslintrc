{
  "extends": "@mntm",
  "parserOptions": {
    "project": "./tsconfig.json",
    "tsconfigRootDir": "."
  },
  "rules": {
    "no-inline-comments": 0,
    "spaced-comment": 0,
    "require-await": 0,
    "@typescript-eslint/require-await": 0,
    "@typescript-eslint/promise-function-async": 0,
    "@typescript-eslint/no-explicit-any": 0
  }
}
