{
  "name": "ahks",
  "version": "0.4.0",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "modern": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./utils": {
      "types": "./lib/utils/index.d.ts",
      "import": "./lib/utils/index.mjs",
      "require": "./lib/utils/index.js"
    }
  },
  "files": [
    "lib",
    "LICENSE",
    "package.json",
    "README.md",
    "utils.d.ts",
    "utils.d.mts",
    "utils.js",
    "utils.mjs"
  ],
  "sideEffects": false,
  "author": {
    "name": "Anton Petrov",
    "email": "im@petrov.engineer",
    "url": "https://petrov.engineer"
  },
  "description": "Utility hooks, functions and constants",
  "license": "MIT",
  "keywords": [
    "ahks",
    "shared",
    "utility",
    "utils",
    "hooks"
  ],
  "homepage": "https://github.com/eolme/ahks",
  "bugs": "https://github.com/eolme/ahks/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/eolme/ahks.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "shx rm -rf lib",
    "build": "npm-run-all --parallel 'build:*'",
    "build:main": "VARIANT=main vite build",
    "build:utils": "VARIANT=utils vite build",
    "lint": "eslint --fix src/**/*.*"
  },
  "devDependencies": {
    "@mntm/eslint-config": "^1.3.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/use-sync-external-store": "^0.0.3",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.4",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-dts": "^1.7.1"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    }
  },
  "dependencies": {
    "use-callback-ref": "^1.3.0",
    "use-composed-ref": "^1.3.0",
    "use-debounce": "^8.0.4",
    "use-deep-compare-effect": "^1.8.1",
    "use-is-mounted-ref": "^1.5.0",
    "use-isomorphic-layout-effect": "^1.1.2",
    "use-latest": "^1.2.1",
    "use-memo-one": "^1.1.3",
    "use-sync-external-store": "^1.2.0"
  }
}
